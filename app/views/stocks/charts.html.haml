
%h1 Stocks#charts
= form_tag(:controller => "stocks", :action => "charts", :method => "get") do
  %p
  =label_tag(:stocks, "stock symbols to rate")
  = text_area_tag(:stocks, @stocks)
  %div= submit_tag("View Charts")

-@stock_symbols.each do |stock|
  = image_tag 'http://chart.finance.yahoo.com/z?s=' + stock + '&t=5y&q=l&l=on&z=l&a=v&p=s&lang=en-US&region=US'
  %hr
